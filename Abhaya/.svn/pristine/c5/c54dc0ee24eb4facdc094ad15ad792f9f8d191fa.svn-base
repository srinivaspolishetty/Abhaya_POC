import { Injectable } from '@angular/core';
import { HttpClient } from '@angular/common/http';
import { environment } from '../../../../environments/environment';

@Injectable({
    providedIn: 'root'
})
export class RouteDeviationService {

    constructor(private httpClient: HttpClient) { }

    getAll(page, size, searchValue, selectDate) {
        const url = `${environment.apiBase}${environment.version}/routeDeviation`;
        return this.httpClient.get(`${url}?page=${page}&size=${size}&searchDate=${selectDate}`);
    }
}
