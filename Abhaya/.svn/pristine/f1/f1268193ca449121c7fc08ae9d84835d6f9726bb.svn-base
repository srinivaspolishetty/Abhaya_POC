<div class="row m-0 p-0 row_shadow">
  <div class="container">
    <div class="topnav">
      <div class="row m-0 p-0">
        <div class="col-11 m-0 p-0">          
          <div class="">
            <a class="icon_alignment text_align" [routerLink]="['/rta/livetracking']">
              <i class="fa fa-map" aria-hidden="true"></i>
              <span>MASTER DASHBOARD</span>
            </a>
          </div>
                <div class="">
            <a class="active icon_alignment text_align" [routerLink]="['/rta/operationalreports']">
              <i class="fa fa-map" aria-hidden="true"></i>
              <span>GOOGLE MAP VIEW</span>
            </a>
          </div>
          <a class="icon_alignment" [routerLink]="['/rta/dashboard']">
            <i class="fa fa-pie-chart" aria-hidden="true"></i>
            <span>DEVICE REPORT</span>
          </a>
    
        </div>
      </div>
    </div>
  </div>
</div>
<div class="row_style">
  <div class="container container_height">
    <nav aria-label="breadcrumb">
      <ol class="breadcrumb">
        <li class="breadcrumb-item active" aria-current="page"><a [routerLink]="['/rta/livetracking']" routerLinkActive="active">Home</a></li>
        <li class="breadcrumb-item" aria-current="page">Google Map View</li>
      </ol>
    </nav>
    <div class="accordian_map m-0 p-0">
      <div class="col-12">
        <div class="panel-group" id="accordion">
          <div class="panel panel-default">
            <div class="panel-heading">
              <div class="panel_title title">
                <a class="title" data-toggle="collapse" data-parent="#accordion" href="#collapseOne">
                  <span class="">
                  </span>Advance Search</a>
              </div>
            </div>
            <div id="collapseOne" class="panel_collapse_alignment panel-collapse collapse in">
              <div class="form-group">
                <select class="form_control_advanced_search  form_select_advanced_search">
                  <option selected="true">Select District</option>
                  <option>Krishna</option>
                  <option>Prakasam</option>
                  <option>Guntur</option>
                  <option>Warangal</option>
                </select>
              </div>
              <div class="form-group">
                <select class="form_control_advanced_search form_select_advanced_search">
                  <option selected="true">Select Mandal</option>
                  <option>Guntur</option>
                  <option>Narasarao Pet</option>
                  <option>Repalle</option>
                  <option>Tenali</option>
                </select>
              </div>
              <div class="form-group">
                <select class="form_control_advanced_search form_select_advanced_search">
                  <option selected="true">Select Police Station</option>
                  <option>Guntur</option>
                  <option>Narasarao Pet</option>
                  <option>Repalle</option>
                  <option>Tenali</option>
                </select>
              </div>
              <div class="search_btn_map">
                <button class="btn btn-primary search_btn_map">Search</button>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="row m-0 p-0">
      <div id="googleMap" class="google_map_align">
        <agm-map [zoom]="10" [latitude]="lat" [longitude]="lng" [disableDefaultUI]="false" [zoomControl]="false" (mapClick)="mapClicked($event)" (mapReady)="mapReady($event)">
          <agm-marker *ngFor="let m of liveVehicleArray; let i = index" (markerClick)="clickedMarker(m.rcNumber, i, $event)" [latitude]="m.latitude"
            [longitude]="m.langitude" [label]="" [markerDraggable]="false" [iconUrl]="iconURL" (dragEnd)="markerDragEnd(m, $event)">
            <agm-info-window>
              <strong>{{m.rcNumber}}</strong>
            </agm-info-window>
          </agm-marker>
        </agm-map>
      </div>
    </div>
  </div>
</div>