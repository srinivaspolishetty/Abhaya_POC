<div class="modal-content popup_width">
  <div class="modal-header popup_size">
    <h5 class="modal-title">Add Administrative</h5>
    <button type="button" class="close close_removeoutline" aria-label="Close" (click)="activeModal.close('Close click')">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
   <form>
  <div class="row popup_size">
  	<div class="admin_select_field">
  		 <div class="form-group">
	        <label class="label_text">Select Type</label>
	          <select class="form-control form_select">
	          <option>State</option>
	          <option>District</option>
	          <option>City</option>
	        </select>
	      </div>
  	</div>

  	<div class="row admin_select_field">
	  <div class="col-6">
	      <div class="form-group">
	        <label class="label_text">District</label>
	        <input type="text" class="form-control form_select" placeholder="Enter District"/>
	      </div>
	    </div>
	    <div class="col-6">
	     <div class="form-group">
	        <label class="label_text">City</label>
	        <input type="text" class="form-control form_select" placeholder="Enter City"/>
	      </div>
	    </div>
	</div>
  	  	<div class="row admin_select_field">
	  <div class="col-6">
	      <div class="form-group">
	        <label class="label_text">State Name</label>
	        <input type="text" class="form-control form_select" placeholder="Enter State"/>
	      </div>
	    </div>
	    <div class="col-6">
	     <div class="form-group">
	        <label class="label_text">State Code</label>
	        <input type="text" class="form-control form_select" placeholder="Enter State Code"/>
	      </div>
	    </div>
	</div>
	  <div class="row m-0 p-0">
      <div class="">
        <button class="btn btn-primary btn_save align-middle">{{isEdit ? 'Update Details' : 'ADD'}}</button>
      </div>
    </div>
  </div>
 
  </form>
</div>