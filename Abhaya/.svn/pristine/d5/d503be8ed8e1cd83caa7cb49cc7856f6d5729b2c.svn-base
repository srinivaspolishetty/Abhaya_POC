export class RouteDeviation {
    public rcNumber: string;
    public location: string;
    public deviationTime: Date;
    public serialNumber: string;
    public dlNumber: string;
    public rfId: string;
    public driverName: string;
    public tripRequestId: string;
    public geoLocation: string;
    public tripId : number;
    public driverContactNumber : number;
    public ownerName : string;
    public ownerContactNumber : number;
    public citizenMobileNumber : number;
    public slocation : string;
    public dlocation : string;

    constructor(tripId : number, rcNumber: string, location: string, deviationTime: Date, serialNumber: string, dlNumber: string, rfId: string,
        driverName: string, tripRequestId: string, geoLocation: string, driverContactNumber : number, ownerName : string, ownerContactNumber : number, citizenMobileNumber : number,
        slocation : string, dlocation : string) {
            this.tripId = tripId;
        this.rcNumber = rcNumber;
        this.location = location;
        this.deviationTime = deviationTime;
        this.serialNumber = serialNumber;
        this.dlNumber = dlNumber;
        this.rfId = rfId;
        this.driverName = driverName;
        this.tripRequestId = tripRequestId;
        this.geoLocation = geoLocation;
        this.driverContactNumber = driverContactNumber;
        this.ownerContactNumber = ownerContactNumber;
        this.ownerName = ownerName;
        this.citizenMobileNumber = citizenMobileNumber;
        this.slocation = slocation;
        this.dlocation = dlocation;
    }
}
