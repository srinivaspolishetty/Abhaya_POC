body {
	line-height: 1.0;
	overflow-x: hidden;
	width: auto;
	margin: opx auto;
	src: url('assets/fonts/GOTHIC.TTF') format('TTF');
	src: url('assets/fonts/GOTHICB.TTF') format('TTF');
