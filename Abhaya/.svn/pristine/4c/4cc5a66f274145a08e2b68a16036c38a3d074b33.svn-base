}

.menu_bar {
	box-align: center;
	margin-top: 10px;
	float: left;
	cursor: pointer;
	font-size: 17px;
}

.bar1,
.bar2,
.bar3 {
	width: 30px;
	height: 2px;
	background-color: #FFF;
	margin-top: 7px;
	margin-bottom: 7px;
	transition: 0.4s;
	margin-left: 0px;
	margin-right: 0px;
}

.change .bar1 {
	-webkit-transform: rotate(-45deg) translate(-9px, 4px);
	transform: rotate(-45deg) translate(-9px, 4px);
}

.change .bar2 {
	opacity: 0;
}

.change .bar3 {
	-webkit-transform: rotate(45deg) translate(-9px, -4px);
	transform: rotate(45deg) translate(-9px, -4px);
}

